#
# Set PATH, otherwise restart-scripts won't find start-stop-daemon
#
PATH=/usr/local/sbin:/usr/local/bin:/sbin:/bin:/usr/sbin:/usr/bin
#
# Regular cron jobs for the syscp package
#
*/5 * * * *	root	/usr/bin/php4 -q -c /etc/php4/syscpcron /var/www/syscp/scripts/cron_tasks.php
0 0 * * *	root	/usr/bin/php4 -q -c /etc/php4/syscpcron /var/www/syscp/scripts/cron_traffic.php

